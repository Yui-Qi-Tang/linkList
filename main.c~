#include "linkList.h"

void main()
{   
				node* list = createHead();
    printf("Add 99, 100 and 101 to list\n");
				addNode(list, 99);
				addNode(list, 100);
				addNode(list, 101);
				dump(list);

				printf("Find 99 in list\n");
				if(isExist(list,99))
				{
				    printf("Got it\n");
				}
				else
				{
				    printf("NO\n");
				}
				printf("Find 1000 in list\n");
    if(isExist(list,1000))
				{
				    printf("Got it!\n");
				}
				else
				{
				    printf("NO\n");
				}
    
				freeList(list);
				
}// End of main   
